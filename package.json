    "@vee-validate/i18n": "^4.7.4",
    "@vee-validate/rules": "^4.7.4",
    "@vueuse/core": "^9.13.0",
    "@vueuse/head": "^1.1.9",
    "@vueuse/nuxt": "^9.13.0",
