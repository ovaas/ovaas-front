{
  "name": "ovaas-front",
  "version": "1.0.12",
  "packageManager": "pnpm@7.0.0",
  "description": "OVAAS Front-end use Vue.js v3",
  "license": "Apache-2.0",
  "repository": "github:ovaas/ovaas-front",
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "release": "npx bumpp --commit --tag --push",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "type-check": "nuxi typecheck"
  },
  "devDependencies": {
    "@hannoeru/eslint-config": "^0.8.7",
    "@iconify/json": "^2.1.41",
    "@intlify/nuxt3": "^0.2.2",
    "@kevinmarrec/nuxt-pwa": "^0.0.2",
    "@types/node": "^17.0.32",
    "@types/nprogress": "^0.2.0",
    "@unocss/nuxt": "^0.33.2",
    "@unocss/reset": "^0.33.2",
    "@vee-validate/i18n": "^4.5.11",
    "@vee-validate/rules": "^4.5.11",
    "@vueuse/core": "^8.4.2",
    "@vueuse/head": "^0.7.6",
    "@vueuse/nuxt": "^8.4.2",
    "axios": "^0.27.2",
    "compressorjs": "^1.1.1",
    "dayjs": "^1.11.2",
    "drauu": "^0.3.0",
    "eslint": "^8.15.0",
    "mitt": "^3.0.0",
    "nuxt": "3.0.0-rc.3",
    "postcss": "^8.4.13",
    "postcss-nested": "^5.0.6",
    "typescript": "^4.6.4",
    "unocss": "^0.33.2",
    "vee-validate": "^4.5.11",
    "vue-i18n": "next"
  },
  "pnpm": {
    "overrides": {
      "@nuxt/kit": "3.0.0-rc.3",
      "@intlify/core-base": "beta"
    },
    "peerDependencyRules": {
      "ignoreMissing": [
        "webpack",
        "vite",
        "vue",
        "vue-router"
      ],
      "allowedVersions": {
        "vue-i18n": "*"
      }
    }
  }
}
