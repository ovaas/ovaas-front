    "@iconify/json": "^2.1.85",
    "@intlify/nuxt3": "^0.2.3",
    "@kevinmarrec/nuxt-pwa": "^0.4.2",
    "@types/node": "^17.0.36",
    "@types/nprogress": "^0.2.0",
    "@unocss/nuxt": "^0.45.1",
    "@unocss/reset": "^0.45.1",
    "@vee-validate/i18n": "^4.6.2",
    "@vee-validate/rules": "^4.6.2",
    "@vueuse/core": "^8.9.4",
    "@vueuse/head": "^0.7.8",
    "@vueuse/nuxt": "^9.0.2",
    "axios": "^0.27.2",
    "compressorjs": "^1.1.1",
    "dayjs": "^1.11.4",
    "dotenv": "^16.0.1",
    "eslint": "^8.20.0",
    "mitt": "^3.0.0",
    "nuxt": "3.0.0-rc.6",
    "postcss": "^8.4.14",
    "postcss-nested": "^5.0.6",
    "typescript": "^4.7.4",
    "unocss": "^0.45.1",
    "vee-validate": "^4.6.2",
    "vue-i18n": "next",
    "zod": "^3.17.10"
