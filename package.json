{
  "name": "ovaas-front",
  "version": "1.0.12",
  "packageManager": "pnpm@7.8.0",
  "description": "OVAAS Front-end use Vue.js v3",
  "license": "Apache-2.0",
  "repository": "github:ovaas/ovaas-front",
  "scripts": {
